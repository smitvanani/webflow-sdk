{
  "name": "@webstudio-is/sdk",
  "version": "0.2.3",
  "description": "Webstudio JavaScript / TypeScript API",
  "author": "Webstudio <github@webstudio.is>",
  "repository": "git@github.com:webstudio-is/webstudio-sdk.git",
  "homepage": "https://webstudio.is",
  "scripts": {
    "build": "yarn build:mdn-data && yarn build:prisma && yarn build:lib",
    "build:mdn-data": "./bin/mdn-data.ts ./src/css",
    "build:prisma": "prisma format && prisma generate",
    "typecheck": "tsc --noEmit",
    "build:lib": "rm -fr lib && tsc",
    "postinstall": "prisma generate",
    "test": "jest",
    "checks": "yarn typecheck && yarn test",
    "prepublishOnly": "yarn typecheck && yarn build"
  },
  "devDependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "@types/css-tree": "^1.0.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "camelcase": "^6.3.0",
    "css-tree": "^2.1.0",
    "esbuild": "^0.14.25",
    "esbuild-jest": "^0.5.0",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-node-loader": "^0.8.0",
    "jest": "^27.5.1",
    "mdn-data": "2.0.23",
    "react": "^17.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@stitches/core": "^1.2.7",
    "immer": "^9.0.12",
    "mitt": "^3.0.0",
    "prisma": "^3.10.0",
    "react-nano-state": "^0.4.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "README.md",
    "lib/*",
    "!*.test.*",
    "prisma/*"
  ],
  "license": "MIT",
  "private": false
}
